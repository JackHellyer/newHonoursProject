<Window x:Class="TimetableCreationTool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimetableCreationTool"
        mc:Ignorable="d"
        Title="Timetable Creation" Height="350" Width="282"
        WindowStartupLocation="CenterScreen">
    <Grid>



        <Button x:Name="newTimetable" Content="New Timetable" HorizontalAlignment="Left" Margin="79,170,0,0" VerticalAlignment="Top" Width="120" Height="25" Click="newTimetable_Click"/>
        <Button x:Name="loadTimetable" Content="Load Timetable" HorizontalAlignment="Left" Margin="79,210,0,0" VerticalAlignment="Top" Width="120" Height="25" Click="loadTimetable_Click"/>
        <Label x:Name="label" Content="Timetable App" Foreground="Black" FontWeight="Bold" FontSize="30"  Margin="24,47,0,0" VerticalAlignment="Top" Height="84" HorizontalAlignment="Left" Width="224"/>



    </Grid>
</Window>
