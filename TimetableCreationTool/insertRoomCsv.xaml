<Window x:Class="TimetableCreationTool.insertRoomCsv"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimetableCreationTool"
        mc:Ignorable="d"
        Title="insertRoomCsv" Height="300" Width="300"
        WindowStartupLocation="CenterOwner">
        
    <Grid>
        <Button x:Name="uploadCSVFiletoDB" Content="Upload CSV file Data" HorizontalAlignment="Left" Margin="149,223,0,0" VerticalAlignment="Top" Width="auto" Click="uploadCsvData_Click"/>
        <Button x:Name="openExternalCSVFile" Content="Open CSV file to add room data" HorizontalAlignment="Left" Margin="42,103,0,0" VerticalAlignment="Top" Width="197" RenderTransformOrigin="0.093,-0.35" Click="openExternalCSVFile_Click"/>

    </Grid>
</Window>
